{
  "name": "crss",
  "version": "1.0.0",
  "description": "An universal React isomorphic boilerplate for building server-side render web app",
  "scripts": {
    "demo-dev": "webpack-dev-server --host 0.0.0.0 --progress --colors --inline --config ./demo/scripts/webpack.demo.client.js",
    "demo-build": "webpack --progress --colors --config ./demo/scripts/webpack.demo.client.js",
    "demo-start": "cross-env NODE_ENV=test nodemon ./demo/server/server.js --watch server --watch build",
    "dev": "webpack-dev-server --host 0.0.0.0 --progress --colors --inline --config ./dev/scripts/webpack.client",
    "dev-build": "webpack --progress --colors --config ./dev/scripts/webpack.client",
    "dev-start": "cross-env NODE_ENV=test nodemon ./demo/myServer.js --watch server --watch build",
    "lint": "eslint . --ext .js",
    "clean": "rimraf ./site/dist/*",
    "test-build": "npm run clean && cross-env NODE_ENV=test webpack --config ./site/scripts/webpack.pro.js --progress",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./site/scripts/webpack.pro.js --progress",
    "start": "node ./site/dist/server/server.js",
    "test-start": "npm run test-build && npm run start",
    "build-start": "npm run build && npm run start",
    "pm2": "pm2"
  },
  "dependencies": {
    "babel-plugin-transform-object-assign": "^6.22.0",
    "ejs": "^2.5.5",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.0",
    "koa-session": "^3.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.0",
    "pm2": "^2.2.2",
    "pwfe-dom": "git+https://github.com/palmg/pwfe-dom.git",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^4.4.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.1.0",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "asset-require-hook": "^1.1.0",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.1",
    "source-map-support": "^0.4.10",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^2.4.5"
  },
  "author": "CRSS",
  "license": "MIT"
}
